<h1>Car Listings</h1>
<ul>
  <%@cars.each do |car| %>
    <li><%= link_to car.make, car_path(car) %></li>
      <% car.photos.each do |photo| %>
        <%=cl_image_tag photo.key, height: 300, width: 400, crop: :fill %>
      <% end %>
    <% end  %>
</ul>


<button><%= link_to "create a listing",new_car_path %></button>
<button><%= link_to "back", root_path  %></button>
