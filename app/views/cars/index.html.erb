<h1>Car Listings</h1>
<ul>
  <%@cars.each do |car| %>
    <li><%= link_to car.make, user_car_file %></li>
    <li><<%= link_to car.model, user_car_file %></li>
    <li><<%= link_to car.price, user_car_file %></li>
  <% end  %>
</ul>


<button><%= link_to "create a listing", new_user_car_path(:id)  %></button>
<button><%= link_to "back", root_path  %></button>
